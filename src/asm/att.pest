WHITESPACE = _{ " " | "\t" }

char  = @{ ASCII_ALPHANUMERIC | "_" }

ident = @{ ASCII_ALPHA ~ (char*) }

register = _{ "%" ~ ident }

label = { ident ~ ":" }

op = @{ ASCII_ALPHA* }
arg = { register | ident }

instruction = { op ~ arg ~ ("," ~ arg)* }

program = _{ (NEWLINE? ~ (label | instruction) ~ NEWLINE+)* }
